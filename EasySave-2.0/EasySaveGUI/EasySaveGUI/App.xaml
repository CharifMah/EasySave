<Application
    x:Class="EasySaveGUI.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:avalonDock="http://schemas.xceed.com/wpf/xaml/avalondock"
    xmlns:local="clr-namespace:EasySaveGUI"
    xmlns:materialIcons="clr-namespace:Material.Icons.WPF;assembly=Material.Icons.WPF"
    StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\CustomButton.xaml" />
                <ResourceDictionary Source="Styles\CustomComboBox.xaml" />
                <ResourceDictionary Source="Styles\CustomSeparator.xaml" />
                <ResourceDictionary Source="Styles\CustomCheckBox.xaml" />
                <ResourceDictionary Source="Styles\CustomSlider1.xaml" />
                <ResourceDictionary Source="Styles\CustomTextBox.xaml" />
                <ResourceDictionary Source="Styles\CustomDataGrid.xaml" />
                <ResourceDictionary Source="Styles\CustomScrollBar.xaml" />
                <ResourceDictionary Source="/AvalonDock.Themes.VS2013;component/DarkBrushs.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
